<tr>
      <td>
        <%= patient.id%>
      </td>
      <td>
        <%= patient.first_name%>
      </td>
      <td>
        <%= patient.last_name%>
      </td>
      <td>
        <%= patient.maternal_surname%>
      </td>
      <td>
        <%= link_to "Editar" , edit_patient_path(patient), id:"edit_#{patient.id}", class: 'btn btn-warning'%>
      </td>
      <td>
        <%= link_to "Eliminar" , patient_path(patient),method: :delete, id:"edit_#{patient.id}", class: 'btn btn-danger '%>
      </td>
      <td>
        <%= link_to "Detalles" , patient_path(patient),method: :delete, id:"edit_#{patient.id}", class: 'btn btn-info '%>
      </td>
</tr>

<tr>
      <td>
        <%= patient.id%>
      </td>
      <td>
        <%= patient.first_name%>
      </td>
      <td>
        <%= patient.last_name%>
      </td>
      <td>
        <%= patient.maternal_surname%>
      </td>
      <td>
        <%= link_to "" , edit_patient_path(patient), id:"edit_#{patient.id}", class: 'btn btn-warning glyphicon glyphicon-pencil'%>
      </td>
      <td>
        <%= link_to "" , patient_path(patient),method: :delete, id:"edit_#{patient.id}", class: 'btn btn-danger glyphicon glyphicon-remove '%>
      </td>
      <td>
        <%= link_to "Info" , patient_path(patient),method: :delete, id:"edit_#{patient.id}", class: 'btn btn-info '%>
      </td>
</tr>
